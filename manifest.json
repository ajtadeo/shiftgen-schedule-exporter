{
  "manifest_version": 3,
  "name": "Schedule Exporter for ShiftGen",
  "description": "Exports schedule with extra information about the provider on staff from ShiftGen to Google Calendar.",
  "version": "2.0",
  "icons": {
    "16": "public/icons/icon_16.png",
    "32": "public/icons/icon_32.png",
    "48": "public/icons/icon_48.png",
    "128": "public/icons/icon_128.png"
  },
  "background": {
    "service_worker": "src/serviceWorker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["src/contentScripts/userContentScript.js"],
      "matches": ["https://www.shiftgen.com/member/multi_site_schedule*"]
    },
    {
      "js": ["src/contentScripts/paContentScript.js"],
      "matches": ["https://www.shiftgen.com/admin/index"]
    },
    {
      "js": ["src/contentScripts/doctorContentScript.js"],
      "matches": ["https://www.shiftgen.com/member/schedule"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://www.shiftgen.com/*"],
      "resources": [
        "src/shiftgen/common.js",
        "src/shiftgen/Scraper.js",
        "src/shiftgen/UserScraper.js",
        "src/shiftgen/PaScraper.js",
        "src/shiftgen/DoctorScraper.js"
      ]
    }
  ],
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "identity",
    "identity.email",
    "sidePanel",
    "tabs"
  ],
  "key": "",
  "oauth2": {
    "client_id": "1000397082507-aulavldtgrqo7rahh1llrpga5mmimnaj.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "email",
      "https://www.googleapis.com/auth/calendar"
    ]
  }
}